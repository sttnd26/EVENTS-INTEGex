<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config></context:annotation-config>
    <bean name="amountDebitedPublisher" class="Model.events.AmountDebitedPublisher"></bean>
    <bean name="amountWithdrawalListener" class="Model.events.AmountWithdrawalListener"></bean>
    <bean name="pinChangePublisher" class="Model.events.PinChangePublisher"></bean>
    <bean name="pinChangeListener" class="Model.events.PinChangeListener"></bean>
    <bean name="phoneChangePublisher" class="Model.events.PhoneChangePublisher"></bean>
    <bean name="phoneChangeListener" class="Model.events.PhoneChangeListener"></bean>

</beans>